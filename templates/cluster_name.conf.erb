[jmx]
username = <%= @jmx_username %>
password = <%= @jmx_password %>
port = <%= @jmx_port %>

[agents]

[cassandra]
seed_hosts = <%=  @cassandra_seed_hosts %>
api_port = <%= @cassandra_api_port %>
username = <%= @cassandra_username %>
password = <%= @cassandra_password  %>
ssl_ca_certs = <%= @cassandra_ssl_ca_certs %>
ssl_client_pem = <%= @cassandra_ssl_client_pem %>
ssl_client_key = <%= @cassandra_ssl_client_key %>

[storage_cassandra]
seed_hosts = <%=  @storage_cassandra_seed_hosts %>
username = <%= @storage_cassandra_username %>
password = <%= @storage_cassandra_password  %>
ssl_ca_certs = <%= @storage_cassandra_ssl_ca_certs %>
ssl_client_pem = <%= @storage_cassandra_ssl_client_pem %>
ssl_client_key = <%= @storage_cassandra_ssl_client_key %>

[agent_config]
ssl_ca_certs = <%= @agent_config_ssl_ca_certs %>
ssl_client_pem = <%= @agent_config_ssl_client_pem %>
ssl_client_key = <%= @agent_config_ssl_client_key %>
